@model IEnumerable<ShipBobShipments.Models.Orders>
@{
    ViewBag.Title = "Index";
}
<h2>Orders List</h2>
<h4>User: @ViewBag.userName</h4>
<p>
    
    @Html.ActionLink("Back to Users", "Index", "Users") |
    @Html.ActionLink("Create New Orders", "Create", "Orders", new { userId = @ViewBag.userId }, null)
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TrackingId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RecipientName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StreetAddress)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.City)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.State)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Zipcode)
        </th>
        <th></th>
    </tr>
@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TrackingId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RecipientName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StreetAddress)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.City)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.State)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Zipcode)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { orderId = item.OrderID }) |
            @Html.ActionLink("Order Details", "Details", new { orderId = item.OrderID }) |
            @Html.ActionLink("Delete", "Delete", new { orderId = item.OrderID })
        </td>
    </tr>
}
</table>
